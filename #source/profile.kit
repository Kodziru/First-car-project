<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>PROFILE</title>
    </head>
    <body>
        <!DOCTYPE html>
        <html lang="fr">
            <head>
                <meta charset="UTF-8" />
                <meta
                    name="description"
                    content="Contactez le vendeur pour avoir plus de info sur votre achat"
                />
                <meta name="keywords" content="HTML, CSS, JavaScript" />
                <meta name="Your Car" content="Les voitures de qualité" />
                <meta
                    name="viewport"
                    content="width=device-width, initial-scale=1.0"
                />
                <link rel="stylesheet" href="./../css/main.css" />

                <title>REGISTER</title>
            </head>
            <body>
                <div class="wrapper register__page">
                    <!--@import 'header.html'-->
                    <main class="profile__main">
                        <section class="section__profile">
                            <div class="img__body__profle">
                                <img
                                    src="/img/1.jpg"
                                    class="profile__img"
                                    alt=""
                                />
                            </div>
                            <div class="abonement__body">
                                <h3 class="abonement">
                                    ABONEMENT “BASIC”<br />
                                    16/08/2022 - 16/09/2022
                                </h3>
                            </div>
                            <div class="table__body">
                                <div class="table__profile" action="">
                                    <div class="profile__name tbl__column">
                                        <p
                                            class="line__bottom"
                                            aria-placeholder="Last Nmae"
                                        >
                                            DOE
                                        </p>
                                        <p
                                            class="line__bottom"
                                            aria-placeholder="First Name"
                                        >
                                            John
                                        </p>
                                    </div>
                                    <div class="profile__contact tbl__column">
                                        <p class="line__bottom">
                                            ark@gmail.com
                                        </p>
                                        <p class="line__bottom">
                                            +33 7 55 55 65 65
                                        </p>
                                    </div>
                                    <div class="profile__adress tbl__column">
                                        <p class="line__bottom">
                                            6 rue Jon Poullart
                                        </p>
                                        <p class="line__bottom">94500</p>
                                        <p class="line__bottom">
                                            Evry sur seine
                                        </p>
                                    </div>
                                </div>
                                <div class="btn__profile__body tbl__column">
                                    <button
                                        type="submit"
                                        class="btn__modify__profile"
                                    >
                                        MODIFIER
                                    </button>
                                </div>
                                <div
                                    class="btn__profile__activities tbl__column"
                                >
                                    <button type="submit" class="profle__btn">
                                        Liste d'achats
                                    </button>
                                    <button type="submit" class="profle__btn">
                                        Vos commontaires
                                    </button>
                                </div>
                            </div>
                        </section>
                    </main>
                    <!--@import 'footer.html'-->
                </div>
                <script src="./js/main.js"></script>
            </body>
        </html>
    </body>
</html>
